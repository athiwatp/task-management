{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "generator-star-spacing": 0,
    "no-param-reassign": ["error", { "props": false }],
    "import/prefer-default-export": 0,
    "no-console": 0,
    "import/no-extraneous-dependencies": [
      "error", {
        "devDependencies": true
      }
    ]
  }
}
