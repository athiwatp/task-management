{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "generator-star-spacing": 0,
    "no-param-reassign": ["error", { "props": false }],
    "import/prefer-default-export": 0,
    "react/jsx-closing-bracket-location": [1, "after-props"],
    "no-console": 0
  }
}
